[run]
source = .
branch = true
omit =
    # Use ** pattern for recursive matching
    **/tests/**
    **/migrations/**
    **/__init__.py
    **/__pycache__/**
    **/static/**
    **/templates/**
    manage.py
    asgi.py
    wsgi.py
    fixtures/**
    requirements/**
    rewardsweb/settings/**

[report]
exclude_lines =
    pragma: no cover
    def __str__
    raise AssertionError
    raise NotImplementedError
    if self.debug:
    if settings.DEBUG
show_missing = true

[xml]
output = coverage.xml